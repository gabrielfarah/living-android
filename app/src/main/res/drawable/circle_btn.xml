<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false" android:state_selected="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_5tran" />
            <stroke android:width="2sp" android:color="@color/soporte" />
            <corners android:radius="30dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark" />
            <stroke android:width="2sp" android:color="@color/blanco" />
            <corners android:radius="30dp"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimaryDark" />
            <stroke android:width="2sp" android:color="@color/blanco" />
            <corners android:radius="30dp"/>
        </shape>
    </item>
</selector>
